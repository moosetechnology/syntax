
SX=c:\sx
ETC=$(SX)/etc
SRC=$(ETC)/src
LIB=$(ETC)/lib
BIB=$(ETC)/etc$(sxV).lib
INCL=$(ETC)/incl

CC=msc /Zi /AL

$(LIB)/sxba_print.obj:	\
			$(SX)/incl/sxunix.h	\
			$(ETC)/force_make 	\
			$(SRC)/sxba_print.c
	$(CC) $(SRC)/sxba_print,$@;
	lib $(BIB)-+$@;

