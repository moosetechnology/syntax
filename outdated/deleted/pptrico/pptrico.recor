Titles
	"",
	"Avertissement:\t",
	"Erreur:\t";

Scanner
	Local
		1 2 3 4	      ; "On supprime le caractère \"" $0 "\", invalide.";
		X 1 2 3 4     ; "On remplace le caractère \"" $0 "\", invalide, par \"" %0 "\".";
		X 0 1 2 3     ; "On insère un caractère \"" %0 "\" devant \"" $0 "\".";

		Dont_Delete = {};
		Dont_Insert = {};

	Global
		Detection     : "On ignore \"%s\".";
		-- parameter: character in error
		Keyword	      : "On ignore ce mot-clef, inconnu.";
		Eol	      : "Fin de ligne";
		Eof	      : "Fin de fichier";
		-- The "character"
		Halt	      : "L'analyse lexicale s'arrête en Fin de fichier.";

Parser
	Local
		0 S 2	      ; "On considère \"" $1 "\" comme une faute d'orthographe du mot-clef \"" %1 "\".";
		S 1	      ; "On considère \"" $0 "\" devant \"" $1 "\" comme une faute d'orthographe du mot-clef \"" %0 "\".";
		0 X 1 2 3     ; "On insère \"" %1 "\" devant \"" $1 "\".";
		0 X 2 3 4     ; "On remplace \"" $1 "\" par \"" %1 "\".";
		0 2 3 4	      ; "On supprime \"" $1 "\".";
		0 X X 1 2 3 4 ; "On insère \"" %1 " " %2 "\" devant \"" $1 "\".";
		X 0 1 2 3     ; "On insère \"" %0 "\" devant \"" $0 " " $1 "\".";
		X 1 2 3 4     ; "On remplace \"" $0 "\" devant \"" $1 "\" par \"" %0 "\".";
		1 2 3 4	      ; "On supprime \"" $0 "\" devant \"" $1 "\".";
		X 2 3 4	      ; "On remplace \"" $0 " " $1 "\" par \"" %0 "\".";
		X X 1 2 3     ; "On remplace \"" $0 "\" devant \"" $1 "\" par \"" %0 " " %1 "\".";

		Dont_Delete = {};
		Dont_Insert = {};

	Forced_Insertion
		"On force \"" %0 "\" devant \"" $1 "\"." ;

	Global
		Key_Terminals = {};
		Validation_Length = 2;
		Followers_Number <= 5
			      : "On attendait \"%s\"^(, \"%s\"^)";
		-- parameters: array (1:Followers_Number) of valid followers at detection point
		Detection     : "Rattrapage global.";
		-- parameters: none
		Restarting    : "L'analyse syntaxique reprend à partir de \"%s\"";
		-- parameters: array (1:Validation_Length) of valid followers at restarting point
		Eof	      : "Fin de fichier";
		-- The "token"
		Halt	      : "L'analyse syntaxique s'arrête en Fin de fichier.";
		-- parameters: none

Abstract
	"On a rendu compte de %d avertissements et %d erreurs.";
	-- parameters: array (1:Titles_No) of number of messages
