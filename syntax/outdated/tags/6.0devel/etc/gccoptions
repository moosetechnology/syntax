GCC_WARNINGS=-Wall -Wextra -Wswitch-default -Wdeclaration-after-statement -Wundef -Wshadow -Wbad-function-cast -Wcast-qual -Wredundant-decls -Wno-char-subscripts -Wcast-align -fno-common -Wstrict-prototypes -Wformat -fstrict-aliasing -Wno-invalid-source-encoding -Wno-unknown-warning-option

COMPILER=gcc $(GCC_WARNINGS) -O2 -Wuninitialized
